	"semanticHighlighting": true, // Enable semantic highlighting
	"semanticTokenColors": {
		// Semantic token colors (used by language servers)
		"comment": {
			"foreground": "#a08968",
			"fontStyle": "normal" // available styles: "italic", "bold", "underline", "strikethrough"
		},
		"string": "#ff9500", // String literals
		"keyword": "#ff6b35", // Language keywords
		"number": "#ffd700", // Numeric literals
		"type": "#9b59b6", // Type names
		"class": "#9b59b6", // Class names
		"function": "#00bfff", // Function names
		"method": "#00bfff", // Method names
		"property": "#ffb347", // Object properties
		"variable": "#ffedc6", // Variable names
		"parameter": "#ffd700", // Function parameters
		"interface": "#9b59b6", // Interface names
		"enum": "#9b59b6", // Enum names
		"enumMember": "#ffd700", // Enum values
		"struct": "#9b59b6", // Struct names
		"typeParameter": "#9b59b6", // Generic type parameters
		"operator": "#ff6b35", // Operators
		"namespace": "#9b59b6", // Namespace names
		"decorator": "#00bfff", // Decorators/Annotations
		"macro": "#00bfff", // Macro names
		"regexp": "#ffb347", // Regular expressions

		// Built-in/Library tokens
		"variable.readonly": "#ffedc6", // Readonly variables
		"variable.defaultLibrary": "#ff6b35", // Built-in variables
		"function.defaultLibrary": "#ff6b35", // Built-in functions
		"method.defaultLibrary": "#ff6b35", // Built-in methods
		"class.defaultLibrary": "#9b59b6", // Built-in classes
		"type.defaultLibrary": "#9b59b6", // Built-in types

		// Special Python tokens
		"selfParameter": {
			"foreground": "#ffd700",
			"fontStyle": "normal"
		},
		"magicFunction": {
			"foreground": "#00bfff",
			"fontStyle": "normal"
		}
	},
