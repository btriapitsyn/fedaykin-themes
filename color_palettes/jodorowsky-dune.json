{
  "name": "jodorowsky-dune",
  "description": "Generated color palette with unified hue, brightness, and saturation",
  "colors": {
    // Core backgrounds
    "color_01": "#12100fff",     // Primary canvas (editor, sidebar, activity bar, tabs, panel)
    "color_02": "#1f0f0bff",     // Status bar, peek view backgrounds
    "color_03": "#2d2623b3",     // Elevated surfaces (line highlight, active tab, widgets, inputs, dropdowns, lists, breadcrumb picker, quick input)
    "color_04": "#2d262477",     // Hover states (list hover, inactive selection)
    "color_05": "#4c4036ff",     // Selection backgrounds
    "color_06": "#54413cff",     // Active selections (autocomplete, quick input focus)

    // Borders and dividers
    "color_07": "#63625C",     // Primary borders (whitespace, ruler, widget borders, input borders)
    "color_08": "#A18B71",     // Line numbers, activity bar icons, terminal bright black, brackets

    // Text hierarchy
    "color_09": "#D5C09D",     // Primary text (editor, cursor, sidebar, active elements, variable semantic)
    "color_10": "#A18B71",     // Secondary text (status bar, inactive tabs, breadcrumb, descriptions, language methods)
    "color_11": "#706a69ff",     // Tertiary text (comments, placeholders)
    "color_12": "#63625C",     // Git ignored files
    "color_13": "#9bc19eff",     // Special variable color (TextMate only)

    // Syntax highlighting
    "color_14": "#93A394",     // Strings, bracket pair 5
    "color_15": "#CA9957",     // Numbers, constants, parameters, tags, bracket pair 6
    "color_16": "#5E99A1",     // Keywords, operators, built-in variables/functions, bracket pair 3
    "color_17": "#e1905aff",     // Functions, methods, decorators, git conflicts, bracket pair 4
    "color_18": "#3D839E",     // Types, classes, interfaces, bracket pair 2
    "color_19": "#9B7386",     // Properties, regex, attributes, bracket pair 1

    // Semantic state colors
    "color_20": "#dc6255ff",     // Errors (squiggles, icons, rulers, mismatched brackets)
    "color_21": "#CA9957",     // Warnings (squiggles, icons, rulers, git modified)
    "color_22": "#5696b8ff",     // Info (squiggles, icons, rulers)
    "color_23": "#689480ff",     // Success, git added, terminal bright green, JS string literals
    "color_24": "#c77f4fff",     // Git untracked

    // UI accent colors
    "color_25": "#5E99A1",     // Primary accent (badges, buttons, links, peek borders, settings modified)
    "color_26": "#76988F",     // Button hover

    // Terminal special
    "color_27": "#ae8297ff",     // Terminal bright magenta
    "color_28": "#5E99A1",     // Terminal bright cyan
    "color_29": "#D5C09D",     // Terminal bright white

    // Transparencies and overlays
    "color_30": "#00000000",   // Invisible borders
    "color_31": "#63625C00",   // Transparent line highlight border
    "color_32": "#63625C55",   // Semi-transparent borders
    "color_33": "#926A5DAA",   // Selection highlight inactive
    "color_34": "#63625C33",   // Scrollbar track, bracket match background
    "color_35": "#926A5DAA",   // Scrollbar hover
    "color_36": "#A18B71CC",   // Scrollbar active
    "color_37": "#5E99A155",   // Active tab border
    "color_38": "#00000055",   // Shadow
    "color_39": "#352D2A22",   // Markdown fenced code block
    "color_40": "#5E99A133",   // Find range highlight
    "color_41": "#5E99A144",   // Word highlight, focus border
    "color_42": "#CA995755",   // Find match highlight
    "color_43": "#CA995788",   // Find match current
    "color_44": "#306E9055",   // Bracket match border
    "color_45": "#5F877533",   // Diff inserted background
    "color_46": "#912C2033"    // Diff removed background
  }
}
