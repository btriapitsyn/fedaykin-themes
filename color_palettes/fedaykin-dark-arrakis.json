{
  "name": "Fedaykin Dark Arrakis",
  "description": "Desert-inspired theme with warm earth tones from the sands of Arrakis",
  "colors": {
    // Core backgrounds - Using the darkest desert night colors
    "color_01": "#0d0906",     // Smoky black - Primary canvas (editor, sidebar, activity bar, tabs, panel)
    "color_02": "#111010",     // Night 2 - Status bar, peek view backgrounds
    "color_03": "#291810",     // Bistre - Elevated surfaces (line highlight, active tab, widgets, inputs, dropdowns, lists, breadcrumb picker, quick input)
    "color_04": "#311e1a",     // Bistre (darker) - Hover states (list hover, inactive selection)
    "color_05": "#403427",     // Taupe - Selection backgrounds
    "color_06": "#4c210f",     // Seal brown - Active selections (autocomplete, quick input focus)
    
    // Borders and dividers - Earth tones
    "color_07": "#4e2e23",     // Caput mortuum - Primary borders (whitespace, ruler, widget borders, input borders)
    "color_08": "#7a6954",     // Coyote - Line numbers, activity bar icons, terminal bright black, brackets
    
    // Text hierarchy - Sand and ash colors
    "color_09": "#c1c0b2",     // Ash gray - Primary text (editor, cursor, sidebar, active elements, variable semantic)
    "color_10": "#858d7c",     // Battleship gray - Secondary text (status bar, inactive tabs, breadcrumb, descriptions, language methods)
    "color_11": "#7a6954",     // Coyote - Tertiary text (comments, placeholders)
    "color_12": "#4e2e23",     // Caput mortuum - Git ignored files
    "color_13": "#9e8e60",     // Lion 2 - Special variable color (TextMate only)
    
    // Syntax highlighting - Desert flora and spice colors
    "color_14": "#b7b38a",     // Sage - Strings, bracket pair 5
    "color_15": "#af5b33",     // Brown sugar - Numbers, constants, parameters, tags, bracket pair 6
    "color_16": "#757c67",     // Reseda green - Keywords, operators, built-in variables/functions, bracket pair 3
    "color_17": "#e8b261",     // Earth yellow - Functions, methods, decorators, git conflicts, bracket pair 4
    "color_18": "#b19660",     // Lion - Types, classes, interfaces, bracket pair 2
    "color_19": "#93a69f",     // Ash gray 2 - Properties, regex, attributes, bracket pair 1
    
    // Semantic state colors - Spice and warning tones
    "color_20": "#af5b33",     // Brown sugar - Errors (squiggles, icons, rulers, mismatched brackets)
    "color_21": "#e8b261",     // Earth yellow - Warnings (squiggles, icons, rulers, git modified)
    "color_22": "#93a69f",     // Ash gray 2 - Info (squiggles, icons, rulers)
    "color_23": "#757c67",     // Reseda green - Success, git added, terminal bright green, JS string literals
    "color_24": "#b7b38a",     // Sage - Git untracked
    
    // UI accent colors - Warm desert accents
    "color_25": "#936230",     // Raw umber - Primary accent (badges, buttons, links, peek borders, settings modified)
    "color_26": "#9c7846",     // Chamoisee - Button hover
    
    // Terminal special - Desert sky and oasis colors
    "color_27": "#b19660",     // Lion - Terminal bright magenta
    "color_28": "#93a69f",     // Ash gray 2 - Terminal bright cyan
    "color_29": "#d7f4f2",     // Mint green - Terminal bright white
    
    // Transparencies and overlays
    "color_30": "#0d090600",   // Invisible borders
    "color_31": "#29181000",   // Transparent line highlight border
    "color_32": "#29181050",   // Semi-transparent borders
    "color_33": "#40342766",   // Selection highlight inactive
    "color_34": "#4e2e2350",   // Scrollbar track, bracket match background
    "color_35": "#4e2e2380",   // Scrollbar hover
