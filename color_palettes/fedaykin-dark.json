{
  "name": "Fedaykin Dark",
  "description": "Unified color palette with all unique colors from the theme",
  "colors": {
    // Core backgrounds
    "color_01": "#0D0907",     // Primary canvas (editor, sidebar, activity bar, tabs, panel)
    "color_02": "#1C1C1C",     // Status bar, peek view backgrounds
    "color_03": "#2A2A2A",     // Elevated surfaces (line highlight, active tab, widgets, inputs, dropdowns, lists, breadcrumb picker, quick input)
    "color_04": "#20201eff",     // Hover states (list hover, inactive selection)
    "color_05": "#2E2E2C",     // Selection backgrounds
    "color_06": "#393936ff",     // Active selections (autocomplete, quick input focus)

    // Borders and dividers
    "color_07": "#3A3A38",     // Primary borders (whitespace, ruler, widget borders, input borders)
    "color_08": "#5A5A58",     // Line numbers, activity bar icons, terminal bright black, brackets

    // Text hierarchy
    "color_09": "#D4D4D2",     // Primary text (editor, cursor, sidebar, active elements, variable semantic)
    "color_10": "#9A9A98",     // Secondary text (status bar, inactive tabs, breadcrumb, descriptions, language methods)
    "color_11": "#7A7A78",     // Tertiary text (comments, placeholders)
    "color_12": "#6A6A68",     // Git ignored files
    "color_13": "#c9c9a7",     // Special variable color (TextMate only)

    // Syntax highlighting
    "color_14": "#7FA872",     // Strings, bracket pair 5
    "color_15": "#B07A7A",     // Numbers, constants, parameters, tags, bracket pair 6
    "color_16": "#6A8CAF",     // Keywords, operators, built-in variables/functions, bracket pair 3
    "color_17": "#C09060",     // Functions, methods, decorators, git conflicts, bracket pair 4
    "color_18": "#9B7BA8",     // Types, classes, interfaces, bracket pair 2
    "color_19": "#5A9999",     // Properties, regex, attributes, bracket pair 1

    // Semantic state colors
    "color_20": "#BD8D8D",     // Errors (squiggles, icons, rulers, mismatched brackets)
    "color_21": "#BDB98D",     // Warnings (squiggles, icons, rulers, git modified)
    "color_22": "#8D9DBD",     // Info (squiggles, icons, rulers)
    "color_23": "#8DBD9D",     // Success, git added, terminal bright green, JS string literals
    "color_24": "#8DAD8D",     // Git untracked

    // UI accent colors
    "color_25": "#8D9DAD",     // Primary accent (badges, buttons, links, peek borders, settings modified)
    "color_26": "#9DADBD",     // Button hover

    // Terminal special
    "color_27": "#C092D0",     // Terminal bright magenta
    "color_28": "#6BC4C4",     // Terminal bright cyan
    "color_29": "#FFFFFF",     // Terminal bright white

    // Transparencies and overlays
    "color_30": "#0D090700",   // Invisible borders
    "color_31": "#2A2A2A00",   // Transparent line highlight border
    "color_32": "#2A2A2A50",   // Semi-transparent borders
    "color_33": "#2E2E2C66",   // Selection highlight inactive
    "color_34": "#3A3A3850",   // Scrollbar track, bracket match background
    "color_35": "#3A3A3880",   // Scrollbar hover
    "color_36": "#3A3A38AA",   // Scrollbar active
    "color_37": "#D4D4D270",   // Active tab border
    "color_38": "#00000030",   // Shadow
    "color_39": "#00000050",   // Markdown fenced code block
    "color_40": "#8D9DAD10",   // Find range highlight
    "color_41": "#8D9DAD16",   // Word highlight, focus border
    "color_42": "#8D9DAD20",   // Find match highlight
    "color_43": "#8D9DAD30",   // Find match current
    "color_44": "#8D9DAD50",   // Bracket match border
    "color_45": "#8DBD9D20",   // Diff inserted background
    "color_46": "#BD8D8D20"    // Diff removed background
  }
}
