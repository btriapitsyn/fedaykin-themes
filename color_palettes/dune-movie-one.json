    "color_17": "#C19A6B",     // Functions, methods, decorators, git conflicts, bracket pair 4
    "color_18": "#78A1B5",     // Types, classes, interfaces, bracket pair 2
    "color_19": "#E5C7A9",     // Properties, regex, attributes, bracket pair 1

    // Semantic state colors
    "color_20": "#C16553",     // Errors (squiggles, icons, rulers, mismatched brackets)
