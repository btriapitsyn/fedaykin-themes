    "color_22": "#78A1B5",     // Info (squiggles, icons, rulers)
    "color_23": "#8FAA7B",     // Success, git added, terminal bright green, JS string literals
    "color_24": "#A66847",     // Git untracked

    // UI accent colors
    "color_25": "#8FAA7B",     // Primary accent (badges, buttons, links, peek borders, settings modified)
