    "color_09": "#EEE7D3",     // Primary text (editor, cursor, sidebar, active elements, variable semantic)
    "color_10": "#D5D2CB",     // Secondary text (status bar, inactive tabs, breadcrumb, descriptions, language methods)
    "color_11": "#9F8A73",     // Tertiary text (comments, placeholders)
    "color_12": "#6F6F69",     // Git ignored files
    "color_13": "#AFA698",     // Special variable color (TextMate only)

