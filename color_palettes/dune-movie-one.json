{
  "name": "dune-movie-one",
  "description": "Generated color palette with unified hue, brightness, and saturation",
  "colors": {
    // Core backgrounds
    "color_01": "#1A1917",     // Primary canvas (editor, sidebar, activity bar, tabs, panel)
    "color_02": "#232B2E",     // Status bar, peek view backgrounds
